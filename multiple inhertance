 class sothumuta{
     void land(){
         System.out.println("100 acer");
     }
 }
 class nisha extends sothumuta{
     void money(){
         System.out.println("100 cr");
     }
 }
 class achu extends nisha{
     void currency(){
         System.out.println("100 doller");
     }
 }
  class nakul extends achu{
     void gold(){
         System.out.println("100 kg");
     }
 }
 public class Main{
     public static void main(String[]args){
         nakul n = new nakul();
         n.land();
         n.money();
         n.currency();
         n.gold();
     }
 }
