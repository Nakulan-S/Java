//Reverse Method in array without using inbuilt function 

 import java.util.*;
 import java.util.Scanner;
 class achu
 {
     public static void main(String[]args){
         Scanner sc = new Scanner(System.in);
         System.out.print("Enter the size of row; ");
         int a = sc.nextInt();
         int []m = new int [a];
         for(int i=a-1;i>=0;i--)
        {
            m[i]=sc.nextInt();
 }
         for(int i=0;i<a;i++)
        {
            System.out.print(m[i]+" ");}}
 }
