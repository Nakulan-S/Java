//Sum of Natural NUmbers 

import java.util.*;
import java.util.Scanner;
class main{
    public static void main(String[] args){
        Scanner sc= new Scanner(System.in);
        int n=sc.nextInt();
        int j=0;
        for(int i=0;i<=n;i++)
        {
            j=j+i;
        }
        System.out.println("the sum of natural numbers are :"+j);
    }
}
